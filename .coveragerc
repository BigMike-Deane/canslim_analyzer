[run]
source = .
omit =
    tests/*
    venv/*
    */venv/*
    */site-packages/*
    setup.py
    */migrations/*
    */config/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

precision = 2

[html]
directory = htmlcov
